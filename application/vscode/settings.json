            "after": [ "<leader>", "<leader>", "F" ]
        },
        {
            "before": [ "<space>" ],
            "commands": [ "whichkey.show" ],
            "silent": true
        },
        {
            "before": [ "/" ],
            "commands": [ "actions.find" ],
            "silent": true
        },
        {
            "before": [ "enter" ],
            "commands": [ "lineBreakInsert" ]
        },
        {
            "before": [ "K" ],
            "commands": [ "editor.action.showHover" ]
        },
        // 转到定义
        {
            "before": [ "g", "d" ],
            "commands": [ "editor.action.revealDefinition" ]
        },
        // 转到引用
        {
            "before": [ "g", "r" ],
            "commands": [ "editor.action.goToReferences" ]
        },
        // 转到实现
        {
            "before": [ "g", "i" ],
            "commands": [ "editor.action.goToImplementation" ]
        },
        // 回退到上一个位置
        {
            "before": [ "g", "b" ],
            "commands": [ "workbench.action.navigateBack" ]
        },
        {
            "before": [ "X" ],
            "commands": [ "workbench.action.closeAllEditors" ]
        },
        {
            "before": [ "H" ],
            "commands": [ "workbench.action.navigateBack" ]
        },
        {
            "before": [ "L" ],
            "commands": [ "workbench.action.navigateForward" ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [ "<space>" ],
            "commands": [ "whichkey.show" ],
            "silent": true
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [ "<space>" ],
            "commands": [ "whichkey.show" ],
            "silent": true
        },
        // format selection
        {
            "before": [ "/" ],
            "commands": [ "actions.findWithSelection", "workbench.action.focusFindWidget" ],
            "silent": true
        },
        {
            "before": [ "f" ],
            "commands": [ "editor.action.formatSelection" ],
            "silent": true
        },
        // expand seletion
        {
            "before": [ "l" ],
            "commands": [ "editor.action.smartSelect.grow" ]
        },
        // shrink selection
        {
            "before": [ "h" ],
            "commands": [ "editor.action.smartSelect.shrink" ]
        },
        // stage selection
        {
            "before": [ "s" ],
            "commands": [ "git.stageSelectedRanges" ]
        },
        // revert selection
        {
            "before": [ "r" ],
            "commands": [ "git.revertSelectedRanges" ]
        },
        // Bind > and < in visual mode to indent/outdent lines (repeatable):
        {
            "before": [ ">" ],
            "commands": [ "editor.action.indentLines" ]
        },
        {
            "before": [ "<" ],
            "commands": [ "editor.action.outdentLines" ]
        }
    ],
